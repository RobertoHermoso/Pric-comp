		<script>
window.addEventListener('load', function () {
            var starRating = $('#filters').find(".headline:contains('Valoración media de los clientes')").closest('fieldset');
            starRating.addClass('snk-rating-stars');
            var li = starRating.find('ul.options li:visible');
            li.find(".facet-text:contains('1.0')").closest('li').addClass('snk-rating-stars-1');
            li.find(".facet-text:contains('2.0')").closest('li').addClass('snk-rating-stars-2');
            li.find(".facet-text:contains('3.0')").closest('li').addClass('snk-rating-stars-3');
            li.find(".facet-text:contains('4.0')").closest('li').addClass('snk-rating-stars-4');
            li.find(".facet-text:contains('5.0')").closest('li').addClass('snk-rating-stars-5');
            $('<div class="rating rating-5"><div class="value-5"></div></div>').insertAfter($('.snk-rating-stars-5').find('input'));
            $('<div class="rating rating-4"><div class="value-4"></div></div>').insertAfter($('.snk-rating-stars-4').find('input'));
            $('<div class="rating rating-3"><div class="value-3"></div></div>').insertAfter($('.snk-rating-stars-3').find('input'));
            $('<div class="rating rating-2"><div class="value-2"></div></div>').insertAfter($('.snk-rating-stars-2').find('input'));
            $('<div class="rating rating-1"><div class="value-1"></div></div>').insertAfter($('.snk-rating-stars-1').find('input'));
            var ratingCounter1 = li.find(".facet-text:contains('1.0')").contents().filter(function () {
                return this.nodeType === 3;
            });
            var ratingCounter2 = li.find(".facet-text:contains('2.0')").contents().filter(function () {
                return this.nodeType === 3;
            });
            var ratingCounter3 = li.find(".facet-text:contains('3.0')").contents().filter(function () {
                return this.nodeType === 3;
            });
            var ratingCounter4 = li.find(".facet-text:contains('4.0')").contents().filter(function () {
                return this.nodeType === 3;
            });
            var ratingCounter5 = li.find(".facet-text:contains('5.0')").contents().filter(function () {
                return this.nodeType === 3;
            });
                var ratingCounter1Number = 0;
                if (ratingCounter1.length > 0) {
                    ratingCounter1Number = ratingCounter1[0].data.match(/&#92;(([^)]+)&#92;)/g);
                }
                var ratingCounter2Number = 0;
                if (ratingCounter2.length > 0) {
                    ratingCounter2Number = ratingCounter2[0].data.match(/&#92;(([^)]+)&#92;)/g);
                }
                var ratingCounter3Number = 0;
                if (ratingCounter3.length > 0) {
                    ratingCounter3Number = ratingCounter3[0].data.match(/&#92;(([^)]+)&#92;)/g);
                }
                var ratingCounter4Number = 0;
                if (ratingCounter4.length > 0) {
                    ratingCounter4Number = ratingCounter4[0].data.match(/&#92;(([^)]+)&#92;)/g);
                }
                var ratingCounter5Number = 0;
                if (ratingCounter5.length > 0) {
                    ratingCounter5Number = ratingCounter5[0].data.match(/&#92;(([^)]+)&#92;)/g);
                }
                li.find(".facet-text:contains('1.0')").append('<em class="ft-counter">'+ratingCounter1Number[0]+'</em>');
                li.find(".facet-text:contains('2.0')").append('<em class="ft-counter">'+ratingCounter2Number[0]+'</em>');
                li.find(".facet-text:contains('3.0')").append('<em class="ft-counter">'+ratingCounter3Number[0]+'</em>');
                li.find(".facet-text:contains('4.0')").append('<em class="ft-counter">'+ratingCounter4Number[0]+'</em>');
                li.find(".facet-text:contains('5.0')").append('<em class="ft-counter">'+ratingCounter5Number[0]+'</em>');
            ratingCounter1.remove();
            ratingCounter2.remove();
            ratingCounter3.remove();
            ratingCounter4.remove();
            ratingCounter5.remove();
	});
   </script>

