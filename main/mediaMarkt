    (function () {
        var element = document.getElementById("my-account-spainclub");
        if (typeof (element) !== 'undefined' && element !== null) {
            element.className = element.className.replace(/\binactive\b/g, "");
        }
    })();
    // Remove options in dropdown mis pedidos

    function removeOption(value) {
        var selectobject = document.getElementById("order_period");
        if (typeof selectobject != "undefined" && selectobject != null) {
            for (var i = 0; i < selectobject.length; i++) {
                if (selectobject.options[i].value == value) selectobject.remove(i);
            }
        }
    }

    removeOption('YEARS_2017');
    removeOption('YEARS_2016');
    removeOption('YEARS_2015');
    removeOption('YEARS_2014');
    removeOption('YEARS_2013');
    removeOption('YEARS_2012');
    removeOption('YEARS_2011');
    removeOption('YEARS_2010');

    // remove element in "Mis pedidos"
    $('#my-account-myordersaccount').closest('li').remove();
    $('p.wishlist').closest('li').remove();
</script>

